<div class="container magazine-wrap">
  <div class="row">
    <% @magazines.each do |magazine| %>
    <div class="col-sm-3 magazine">
      <article>
        <%= link_to image_tag(magazine.cover.w800_h600_fl_q80, class: 'img-responsive'), magazine.file.url, target: '_blank' %>
        <h3 class="title"><%= link_to magazine.issue(I18n.locale), magazine.file.url, target: '_blank' %></h3>
      </article>
    </div>
    <% end %>
  </div>
</div>